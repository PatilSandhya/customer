<script> 
    const showSidebarCurrentTab = (id, elementPos) => {
        $(`#${id}`).prev().attr('aria-expanded', true).attr('data-active', true);
        $(`#${id}`).addClass('show');
        if(elementPos) $(`#${id} li:nth-child(${elementPos})`).first().addClass('active');
    }
    $(document).ready(  () => {
        sidebarViewByRole();
    })
    
    const sidebarViewByRole = () =>{
        const role = localStorage.getItem('role');
        //alert("this is role" + role);
        if(role == "AC"){
            //document.getElementById("customerData").style.visibility = "hidden";
